.logo-wrapper {
  width: var(--sidebar-width);
  height: auto;
  resize: horizontal;
  position: relative;
  cursor: pointer;
  background: var(--springWood);

  &_collapsed {
    width: var(--sidebar-width-collapsed);
    display: flex;
    align-items: center;
    justify-content: center;
    box-sizing: border-box;
    padding: 1.5rem 0;
    background: var(--springWood);
  }
  & svg {
    margin-top: 4px;
    margin-left: 15px;
  }
}
