{
  "name": "game",
  "version": "1.0.0",
  "description": "Game project by New Vasuyki team",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production npm-run-all --print-label build start:server",
    "start:server": "node ./build/server-build/server/index.js",
    "watch:server": "nodemon --watch build ./build/server-build/server/index.js",
    "watch:build": "tsc --project server --watch",
    "build": "npm-run-all clean build:*",
    "build:server": "tsc --project server",
    "build:client": "webpack --config webpack-configs/client.config.ts",
    "build:ssr": "webpack --config webpack-configs/ssr.client.config.ts",
    "watch:ssr": "webpack --mode development --watch --config ./webpack-configs/ssr.client.config.ts",
    "dev": "cross-env NODE_ENV=development SKIP_FORUM_AUTH=1 npm-run-all --print-label clean build:server --parallel watch:ssr start:server",
    "dev:watch": "cross-env NODE_ENV=development SKIP_FORUM_AUTH=1 npm-run-all --print-label clean build:server -p watch:build watch:ssr watch:server",
    "dev:forumWithAuth": "cross-env NODE_ENV=development SKIP_FORUM_AUTH=0 npm-run-all --print-label clean build:server build:ssr -p watch:build watch:ssr watch:server",
    "eslint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint": "stylelint ./src/**/*.pcss && eslint \"src/**/*.{ts,tsx}\"",
    "lint-fix": "stylelint --fix ./src/**/*.pcss && eslint --fix \"src/**/*.{ts,tsx}\"",
    "test": "jest",
    "clean": "rimraf ./build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newvasyuki/game.git"
  },
  "author": "New Vasyuki",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/newvasyuki/game/issues"
  },
  "homepage": "https://github.com/newvasyuki/game#readme",
